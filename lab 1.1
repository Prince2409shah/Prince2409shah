//PROGRAM TO CALCULATE SIMPLE INTEREST. 
#include<stdio.h>
int main()
{
    int n;
    float p,r,a;
    printf("Enter the value of principal amount");
    scanf("%f",&p);
    printf("Enter the value of rate of intrest");
    scanf("%f",&r);
    printf("Enter the value of number of year");
    scanf("%d",&n);
    a=p*r*n/100;
    printf("The value of simple intrest is %f",a);
    return 0;
}
