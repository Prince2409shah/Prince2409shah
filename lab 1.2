//PROGRAM TO READ MARKS OF FIVE SUBJECT OF A STUDENT AND CALCULATE TOTAL AND PERCENTAGE. 
#include<stdio.h>
int main()
{
    int a,b,c,d,e,t;
    float p;
    t=a+b+c+d+e;
    p=(t/500)*100;
    printf("Enter the marks in 5 subject");
    scanf("%d %d %d %d %d ",&a,&b,&c,&d,&e);
    printf("The total of the marks is %d",t);
    printf("The percentage of student according to given marks is %d",p);
    return 0;
}
